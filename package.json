{
  "name": "react-redux-workflow",
  "version": "0.1.0",
  "description": "a front end workflow (using ES6) with react, redux, react-router and rethunk.",
  "keywords": [
    "ES6",
    "babel",
    "react",
    "redux",
    "rethunk",
    "webpack"
  ],
  "author": "luoyi",
  "license": "ISC",
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "dist": "better-npm-run dist"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "8080",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config configs/webpack.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "dist": {
      "command": "nodemon --exec babel-node bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": "8080",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "better-npm-run": "0.0.8",
    "clean-webpack-plugin": "^0.1.6",
    "css-loader": "^0.23.0",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "json-loader": "^0.5.4",
    "nib": "^1.1.0",
    "postcss-loader": "^0.9.0",
    "redbox-react": "^1.2.4",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0",
    "yargs": "^4.0.0"
  }
}
