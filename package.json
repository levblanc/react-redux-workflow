{
  "name": "fengche-ssms-system",
  "version": "0.1.0",
  "description": "fengche-ssms-system front end workflow (using ES6) with react, redux, react-router and rethunk.",
  "keywords": [
    "ES6",
    "babel",
    "react",
    "redux",
    "rethunk",
    "webpack"
  ],
  "author": "luoyi",
  "license": "ISC",
  "scripts": {
    "mockup-dev": "better-npm-run mockup-dev",
    "mockup-release": "better-npm-run mockup-release",
    "build-mockup-alpha": "better-npm-run build-mockup --env alpha",
    "build-mockup-test": "better-npm-run build-mockup --env test",
    "build-mockup-release": "better-npm-run build-mockup --env release",
    "dev": "better-npm-run dev",
    "dev-beta": "better-npm-run dev-beta --api ENV_BETA",
    "dev-release": "better-npm-run dev-release --api ENV_RELEASE",
    "build-alpha": "better-npm-run build --env alpha",
    "build-test": "better-npm-run build --env test",
    "build-release": "better-npm-run build --env release",
    "dist": "better-npm-run dist",
    "lint": "eslint src bin/server.js"
  },
  "betterScripts": {
    "mockup-dev": {
      "command": "nodemon --exec babel-node bin/server.js",
      "env": {
        "NODE_ENV": "mockup-dev",
        "PORT": "8080",
        "DEBUG": "app:*"
      }
    },
    "build-mockup": {
      "command": "babel-node bin/build.js",
      "env": {
        "NODE_ENV": "mockup-release",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "8080",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "babel-node bin/build.js",
      "env": {
        "NODE_ENV": "release",
        "DEBUG": "app:*"
      }
    },
    "dist": {
      "command": "nodemon --exec babel-node bin/server.js",
      "env": {
        "NODE_ENV": "release",
        "PORT": "8989",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "classnames": "^2.2.5",
    "git-validate": "^2.1.4",
    "react": "15.0.2",
    "react-addons-create-fragment": "^15.1.0",
    "react-date-range": "^0.2.4",
    "react-dom": "15.0.2",
    "react-paginate": "^2.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "superagent": "^1.8.3",
    "superagent-mocker": "^0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "better-npm-run": "0.0.8",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "clean-webpack-plugin": "^0.1.9",
    "compression-webpack-plugin": "^0.3.1",
    "connect-history-api-fallback": "^1.2.0",
    "css-loader": "^0.23.0",
    "debug": "^2.2.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "json-loader": "^0.5.4",
    "minimist-argv": "^1.0.0",
    "nib": "^1.1.0",
    "postcss-loader": "^0.9.0",
    "poststylus": "^0.2.3",
    "redbox-react": "^1.2.4",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "url-loader": "^0.5.6",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
